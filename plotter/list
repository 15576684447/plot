cat $(go env GOROOT)/src/container/list/list.go \
| gofmt -r 'New -> newList' \
| gofmt -r 'List -> list' \
| gofmt -r 'Element -> element' \
| gofmt -r 'interface{} -> point' \
| sed 's/package list/\npackage plotter/g' \
> list.go
